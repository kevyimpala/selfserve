{
  "name": "homecook",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "server"
  ],
  "main": "expo/AppEntry",
  "scripts": {
    "dev": "concurrently -k -n server,web \"npm run dev:server\" \"npm run dev:web -- --localhost\"",
    "dev:app": "expo start",
    "dev:web": "expo start --web",
    "dev:server": "npm run dev --workspace server",
    "typecheck": "tsc --noEmit",
    "typecheck:server": "npm run typecheck --workspace server",
    "build:server": "npm run build --workspace server",
    "ci": "npm run typecheck && npm run typecheck:server && npm run build:server"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "expo": "~52.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.1.2",
    "typescript": "^5.6.3"
  }
}
